{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "projects/**/*"
  ],
  "exclude": [
    "**/*.spec.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "lib": [
      "ES2019",
      "dom"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}